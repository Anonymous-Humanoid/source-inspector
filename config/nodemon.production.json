{
    "exec": "npx ts-node config/webserver.ts",
    "verbose": true,
    "ignore": ["/dist", "/build", "/coverage", "/.vscode", "/.git", "/config"],
    "watch": [
        "./config/webserver.ts",
        "./config/webpack.config.ts",
        "./src/react-app-env.d.ts",
        "./package-lock.json",
        "tsconfig.json",
        ".babelrc"
    ],
    "env": {
        "NODE_ENV": "production",
        "BABEL_ENV": "production",
        "ASSET_PATH": "/",
        "OUTPUT_DIR": "dist",
        "PACKAGE_NAME": "source-inspector",
        "PACKAGE_DESCRIPTION": "A from-scratch open-source document source inspector that aims to be invisible to websites",
        "WEBPACK_PORT": "8081"
    },
    "ext": "json,cjs,mjs,js,ts,babelrc"
}
