{
    "exec": "npx ts-node config/webserver.ts",
    "verbose": true,
    "ignore": ["/dist", "/build", "/coverage", "/.vscode", "/.git", "/config"],
    "watch": [
        "./config/webserver.ts",
        "./config/webpack.config.ts",
        "./src/react-app-env.d.ts",
        "package-lock.json",
        "tsconfig.json",
        ".babelrc"
    ],
    "env": {
        "NODE_ENV": "development",
        "BABEL_ENV": "development",
        "ASSET_PATH": "/",
        "OUTPUT_DIR": "build",
        "PACKAGE_NAME": "(DEV BRANCH) source-inspector",
        "PACKAGE_DESCRIPTION": "DEVELOPER USE ONLY - DO NOT PUBLISH",
        "WEBPACK_PORT": "8080"
    },
    "ext": "json,cjs,mjs,js,ts,babelrc"
}
