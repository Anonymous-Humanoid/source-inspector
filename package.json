{
    "name": "source-inspector",
    "version": "1.0.0",
    "description": "A from-scratch open-source document source inspector that aims to be invisible to websites",
    "license": "MIT",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/Anonymous-Humanoid/source-inspector.git"
    },
    "scripts": {
        "start": "npm run start:dev",
        "start:dev": "nodemon --config config/nodemon.development.json",
        "start:prod": "nodemon --config config/nodemon.production.json",
        "prettier": "prettier --write **/*.{cjs,mjs,js,jsx,ts,tsx,json,css,scss,html} --config config/.prettierrc --ignore-path config/.prettierignore",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "dependencies": {
        "async-mutex": "^0.5.0",
        "form-data": "^4.0.3",
        "https": "^1.0.0",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "uuid": "^11.1.0"
    },
    "devDependencies": {
        "@types/chrome": "^0.0.309",
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "clean-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^13.0.0",
        "css-loader": "^7.1.2",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-react": "^7.37.4",
        "eslint-plugin-react-hooks": "^4.6.0",
        "generate-json-webpack-plugin": "^2.0.0",
        "html-loader": "^4.2.0",
        "html-webpack-plugin": "^5.6.3",
        "nodemon": "^3.1.9",
        "prettier": "^2.8.3",
        "sass": "^1.57.1",
        "sass-loader": "^13.2.0",
        "source-map-loader": "^3.0.1",
        "style-loader": "^3.3.1",
        "terser-webpack-plugin": "^5.3.14",
        "ts-loader": "^9.5.2",
        "ts-node": "^10.9.2",
        "type-fest": "^3.5.2",
        "typescript": "^5.8.3",
        "webpack": "^5.98.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^5.0.4"
    }
}
